doctype html
html(lang="es")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title InmoLinking | #{pagina}
        block styles
        link(rel="stylesheet", href="/css/app.css") 
    body(class="min-h-scree bg-gray-50")
